{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:worker": "nest start worker",
    "dev": "nest start --watch",
    "dev:worker": "nest start --watch worker",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:prod-worker": "node dist/worker",
    "seed": "ts-node -r tsconfig-paths/register ./src/databases/seeds/main.ts",
    "seed:prod": "node dist/databases/seeds/main.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "pnpm run typeorm -- --dataSource=src/databases/data-source.ts migration:generate",
    "migration:create": "pnpm run typeorm -- --dataSource=src/databases/data-source.ts migration:create",
    "migration:run": "pnpm run typeorm -- --dataSource=src/databases/data-source.ts migration:run",
    "migration:revert": "pnpm run typeorm -- --dataSource=src/databases/data-source.ts migration:revert",
    "typeorm:prod": "node ./node_modules/typeorm/cli.js",
    "migration:run:prod": "pnpm run typeorm:prod -- --dataSource=dist/databases/data-source.js migration:run",
    "migration:revert:prod": "pnpm run typeorm:prod -- --dataSource=dist/databases/data-source.js migration:revert",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --write",
    "format:check": "biome format . ",
    "format:fix": "biome format .  --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.804.0",
    "@aws-sdk/s3-request-presigner": "^3.804.0",
    "@fast-csv/format": "^5.0.2",
    "@nestjs/axios": "^3.1.3",
    "@nestjs/bullmq": "^11.0.2",
    "@nestjs/common": "^10.4.17",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.17",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.17",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^8.1.1",
    "@nestjs/terminus": "^10.3.0",
    "@nestjs/typeorm": "^10.0.2",
    "@sendgrid/mail": "^8.1.5",
    "@sentry/nestjs": "^9.41.0",
    "apple-signin-auth": "^2.0.0",
    "argon2": "^0.43.0",
    "bullmq": "^5.52.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "compression": "^1.8.0",
    "dayjs": "^1.11.13",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "fb": "^2.0.0",
    "google-auth-library": "^9.15.1",
    "graphql": "^16.11.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.15.6",
    "puppeteer": "^24.33.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "twilio": "^5.6.0",
    "typeorm": "^0.3.22",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.17",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.11.22",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.17.31",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^11.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": ["**/*.(t|j)s"],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
